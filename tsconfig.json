{
  "compilerOptions": {
    "baseUrl": "./cypress",
    "target": "ES6",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "types": ["cypress", "node"],
    "paths": {
      "$fixtures/*": ["./fixtures/*"],
      "$support/*": ["./support/*"],
      "$pageObjects/*": ["./support/page_objects/*"],
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
  },
  "include": ["**/*.ts"]
}
